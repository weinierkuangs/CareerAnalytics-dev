@model CareerAnalytics_dev.Models.SurveyInfoModel
@{
    ViewBag.Title = "SurveyInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="~/Content/style.css">
</head>
<body id="UserProfile" style="background-color:darkgrey;">
    <div id="box-profile">
            <div class="form-horizontal">
                <h4>Survey Info</h4>
                <hr />
                <div class="form-group">
                    @Html.LabelFor(model => model.NumberOfSurveyTakenLastWeek, new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.DisplayFor(model => model.NumberOfSurveyTakenLastWeek)
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.NumberOfSurveyTakenOverall, new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.DisplayFor(model => model.NumberOfSurveyTakenOverall)
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.NumberOfUndergradautes, new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.DisplayFor(model => model.NumberOfUndergradautes)
                    </div>
                </div>

            </div>
    </div>
</body>
</html>

@section Scripts {
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
}
